package VerificaNumero;

public class Repeticao {

	public static void main(String[] args) {

		// For => Repita
		// Declarando, Condição, i = i + 1;

		for (int i = 0; i <= 10; i += 2) {
			System.out.println(i);
		}
		for (int i = 0; i <= 6; i++) {
			System.out.println(" ");

			for (int j = 0; j <= 1; j++) {
				System.out.print("*");
			}
		}

	}
}
